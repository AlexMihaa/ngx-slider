{
  "name": "ng5-slider",
  "version": "1.1.0",
  "private": true,
  "description": "Slider component for Angular 5+",
  "keywords": [
    "slider",
    "component",
    "angular",
    "ng",
    "angular2",
    "ng2",
    "angular4",
    "ng4",
    "angular5",
    "ng5"
  ],
  "author": "Piotr Dziwinski",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular-slider/ng5-slider.git"
  },
  "bugs": {
    "url": "https://github.com/angular-slider/ng5-slider/issues"
  },
  "homepage": "https://github.com/angular-slider/ng5-slider#readme",
  "scripts": {
    "prepare": "npm run prepare:lib && npm run prepare:demo-app",
    "prepare:lib": "node scripts/generate-lib-files.js",
    "prepare:demo-app": "node scripts/generate-demo-app-templates.js",
    "start": "npm run prepare && ng serve",
    "build": "npm run build:lib && npm run build:demo-app && npm run build:docs",
    "build:lib": "npm run prepare && rimraf dist/ng5-slider dist/ng5-slider.tgz && ng-packagr -p src/ng5-slider/package.json",
    "build:demo-app": "npm run prepare && rimraf dist/demo-app && ng build --prod --base-href /ng5-slider/",
    "build:docs": "npm run prepare && rimraf docs && npx typedoc --excludeExternals --module umd --exclude '**/*.spec.ts' --exclude '**/public_api.ts' --out docs/ src/ng5-slider/",
    "publish": "scripts/publish-gh-pages.sh",
    "test": "npm run prepare && ng test",
    "test:once": "npm run prepare && ng test --watch=false",
    "lint": "npm run prepare && ng lint",
    "e2e": "npm run prepare && ng e2e",
    "ci": "npm run test:once && npm run e2e && npm run lint && npm run build:lib && npm run build:demo-app"
  },
  "dependencies": {
    "@angular/animations": "^5.0.2",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
    "@angular/core": "^5.0.2",
    "@angular/forms": "^5.0.2",
    "@angular/http": "^5.0.2",
    "@angular/platform-browser": "^5.0.2",
    "@angular/platform-browser-dynamic": "^5.0.2",
    "@angular/router": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler": "^5.0.2",
    "@angular/compiler-cli": "^5.0.2",
    "@angular/language-service": "^5.0.2",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "escape-html": "^1.0.3",
    "fs": "0.0.1-security",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.2",
    "path": "^0.12.7",
    "prismjs": "^1.14.0",
    "protractor": "~5.1.2",
    "protractor-console-plugin": "^0.1.1",
    "ts-node": "~4.1.0",
    "tsickle": "~0.25.5",
    "tslint": "~5.9.1",
    "typedoc": "^0.11.1",
    "typescript": "~2.4.2"
  }
}
